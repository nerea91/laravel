{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"binarix/foundation-pagination": "dev-master",
		"jenssegers/oauth": "dev-master",
		"laravel/framework": "4.2.*",
		"schickling/backup": "dev-master",
		"stolz/assets": "dev-master",
		"stolz/laravel-form-builder": "dev-master",
		"stolz/menu": "dev-master"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "dev-master",
		"daylerees/container-debug": "dev-master",
		"squizlabs/php_codesniffer": "2.*",
		"stolz/laravel-html-tidy": "dev-master",
		"stolz/laravel-schema-spy": "dev-master",
		"way/generators": "3"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/libraries",
			"app/views/composers"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"bin/fix_file_permissions",
			"ctags"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"bin/fix_file_permissions",
			"ctags"
		],
		"post-create-project-cmd": [
			"php artisan key:generate",
			"bin/fix_file_permissions",
			"ctags"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
