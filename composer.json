{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.6.4",
		"laravel/framework": "5.4.*",
		"laravel/socialite": "~2.0",
		"league/commonmark": "dev-master",
		"rap2hpoutre/laravel-log-viewer": "0.3.0",
		"spatie/laravel-tail": "~1.0",
		"stolz/assets": "dev-master",
		"laravelcollective/html": "^5.4",
		"stolz/menu": "dev-master",
		"barryvdh/laravel-snappy": "^0.3.0",
		"maatwebsite/excel": "~2.1.0"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "dev-master",
		"filp/whoops": "~1.0",
		"phpunit/phpunit": "~4.0",
		"squizlabs/php_codesniffer": "~2",
		"stolz/laravel-html-tidy": "dev-master",
		"stolz/laravel-schema-spy": "dev-master",
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0"
	},
	"autoload": {
		"files": [
			"resources/helpers/loader.php"
		],
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		]
	},
	"scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
