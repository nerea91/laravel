{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.5.9",
		"laravel/framework": "5.2.*",
		"laravel/socialite": "~2.0",
		"league/commonmark": "dev-master",
		"rap2hpoutre/laravel-log-viewer": "0.3.0",
		"spatie/laravel-tail": "~1.0",
		"stolz/assets": "dev-master",
		"stolz/laravel-form-builder": "dev-master",
		"stolz/menu": "dev-master"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "dev-master",
		"filp/whoops": "~1.0",
		"phpunit/phpunit": "~4.0",
		"squizlabs/php_codesniffer": "~2",
		"stolz/laravel-html-tidy": "dev-master",
		"stolz/laravel-schema-spy": "dev-master",
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0"
	},
	"autoload": {
		"files": [
			"resources/helpers/loader.php"
		],
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"which ctags && ctags"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize",
			"which ctags && ctags"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
