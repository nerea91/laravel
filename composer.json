{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
    "repositories": [
		{ "type": "vcs", "url": "https://github.com/Stolz/Laravel-Foundation-Pagination" },
		{ "type": "vcs", "url": "https://github.com/Stolz/container-debug" },
		{ "type": "vcs", "url": "https://github.com/Stolz/laravel-backup" },
		{ "type": "vcs", "url": "https://github.com/Stolz/laravel-oauth" },
		{ "type": "vcs", "url": "https://github.com/Stolz/zurb-foundation-laravel" }
	],
	"require": {
		"binarix/foundation-pagination": "dev-master",
		"illuminate/html": "~5.0",
		"illuminate/remote": "~5.0",
		"jenssegers/oauth": "dev-master",
		"laravel/framework": "~5.0",
		"schickling/backup": "dev-master",
		"stolz/assets": "dev-master",
		"stolz/laravel-form-builder": "dev-master",
		"stolz/menu": "dev-master"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "dev-master",
		"daylerees/container-debug": "dev-master",
		"phpunit/phpunit": "~4.0",
		"squizlabs/php_codesniffer": "2.*",
		"stolz/laravel-html-tidy": "dev-develop",
		"stolz/laravel-schema-spy": "dev-develop",
		"way/generators": "dev-master"
	},
	"autoload": {
		"files": [
			"resources/helpers.php"
		],
		"classmap": [
			"database",
			"tests/TestCase.php"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"bin/fix_file_permissions",
			"ctags"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"bin/fix_file_permissions",
			"ctags"
		],
		"post-create-project-cmd": [
			"php artisan key:generate",
			"bin/fix_file_permissions",
			"ctags"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
