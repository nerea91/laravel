{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"league/commonmark": "dev-master",
		"spatie/laravel-tail": "~1.0",
		"stolz/assets": "dev-master",
		"stolz/laravel-form-builder": "dev-master",
		"stolz/menu": "dev-master"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "dev-master",
		"filp/whoops": "~1.0",
		"squizlabs/php_codesniffer": "~2",
		"stolz/laravel-html-tidy": "dev-master",
		"stolz/laravel-schema-spy": "dev-master"
	},
	"autoload": {
		"files": [
			"resources/helpers/loader.php"
		],
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"bin/fix_file_permissions",
			"ctags"
		],
		"pre-update-cmd": [
        		"php artisan clear-compiled"
        	],
		"post-update-cmd": [
			"php artisan optimize",
			"bin/fix_file_permissions",
			"ctags"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate",
			"bin/fix_file_permissions",
			"ctags"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
