{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		"stolz/assets": "dev-master",
		"stevenmaguire/zurb-foundation-laravel": "dev-master",
		"binarix/foundation-pagination": "dev-master",
		"stolz/laravel-html-tidy": "dev-develop"
	},
	"require-dev": {
		"firephp/firephp-core": "dev-master",
		"way/generators": "dev-master",
		"daylerees/container-debug": "dev-master",
		"barryvdh/laravel-debugbar": "dev-master"
	},
	"repositories": [
		{
		"type": "vcs",
		"url": "https://github.com/stolz/zurb-foundation-laravel"
		}
	],
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/libraries",
			"app/views/composers",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize",
			"ctags"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"ctags",
			"php artisan debugbar:publish"
		],
		"post-create-project-cmd": [
			"php artisan key:generate",
			"ctags"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
