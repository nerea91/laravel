{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.1.3",
		"barryvdh/laravel-snappy": "^0.4.0",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.6.*",
		"laravel/tinker": "~1.0",
		"laravel/socialite": "~2.0",
		"laravelcollective/html": "^5.4",
		"league/commonmark": "dev-master",
		"maatwebsite/excel": "~2.1.0",
		"rap2hpoutre/laravel-log-viewer": "0.3.0",
		"shin1x1/laravel-force-https-url-scheme": "^1.1",
		"spatie/laravel-tail": "^3.0.1",
		"stolz/assets": "dev-master",
		"stolz/menu": "dev-master"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "dev-master",
		"nunomaduro/collision": "~1.1",
		"filp/whoops": "~2.0",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~7.0",
		"symfony/thanks": "^1.0",
		"squizlabs/php_codesniffer": "~2",
		"stolz/laravel-html-tidy": "dev-master",
		"stolz/laravel-schema-spy": "dev-master"
	},
	"autoload": {
		"files": [
			"resources/helpers/loader.php"
		],
		"classmap": [
			"database/seeds"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
		   "Tests\\": "tests/"
	   }
	},
	"extra": {
        "laravel": {
            "dont-discover": [
				"laravelcollective/html"
            ]
        }
	},
	"scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
         "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
		]
    },
    "config": {
        "preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
